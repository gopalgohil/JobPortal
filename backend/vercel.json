{
    "version": 2,
    "build":[
        {
            "src": "backend/index.js",
            "use": "@vercel/node",
            "config": {
               "includeFiles":["dist/**"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/backend/index.js"
        }
    ]
}